  #asteroid = [
    x: 300
    y: 250
    vx: 0
    vy: 0
    size: 25
    orientation: 180
  ]

  asteroid-x = #asteroid.x
  asteroid-y = #asteroid.y  
  asteroid-size = #asteroid.size
  #asteroid-drawing = [
      "arc" [center-x: asteroid-x center-y: asteroid-y radius: asteroid-size starting-angle: 0 ending-angle: 360 fill: 0x000000 stroke: 0xFFFFFF line-width: 3]
  ]